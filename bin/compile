#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir> <env-dir>

set -exuo pipefail

BUILD_DIR=${1:-}
CACHE_DIR=${2:-}
ENV_DIR=${3:-}

cd $BUILD_DIR

cd ai/service/text2img
# python -m pip install --upgrade pip
which -a pip || true
which -a pip3 || true
pip3 install -r requirements.txt
cd -

cd data/migrate_posts
yarn install

exit 0
